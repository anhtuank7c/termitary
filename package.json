{
  "name": "termitary",
  "version": "1.0.50",
  "type": "module",
  "engines": {
    "bun": ">=1.3.1"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "worker": "bun run --watch src/worker.ts",
    "start": "bun run src/index.ts",
    "start:worker": "bun run src/worker.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/cron": "^1.4.1",
    "elysia": "latest"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "bun-types": "latest",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2"
  },
  "module": "src/index.js",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
